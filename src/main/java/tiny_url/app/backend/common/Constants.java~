package tiny_url.app.backend.common;

public class Constants {

    public static class RESPONSE_TYPE {
        public static final String SUCCESS = "SUCCESS";
        public static final String ERROR = "ERROR";
        public static final String WARNING = "WARNING";
        public static final String CONFIRM = "CONFIRM";
        public static final String invalidPermission = "invalidPermission";
    }

    public static class RESPONSE_CODE {
        public static final String SUCCESS = "success";
        public static final String ERROR = "error";
        public static final String WARNING = "warning";
        public static final String NOT_FOUND = "not_found";
    }

    public static class RESPONSE_MESSAGE {
        public static final String SUCCESS = "Executed successfully";
        public static final String ERROR = "Executed error";
        public static final String WARNING = "Warning message";
        public static final String FAIL = "Fail message";
    }

    public interface CATEGORY_TYPE {
        String CON_CHO = "DOG";
        String CON_MEO = "CAT";
        String CON_LON = "PIG";
        String CON_BO = "CAO";
    }
}
